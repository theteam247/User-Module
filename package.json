{
  "name": "user-module",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run serve",
    "build": "npm-run-all build-ts tslint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint --fix --project .",
    "debug": "npm-run-all build  watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all tslint lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theteam247/User-Module.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/theteam247/User-Module/issues"
  },
  "homepage": "https://github.com/theteam247/User-Module#readme",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.27",
    "@types/compression": "0.0.36",
    "@types/dot-object": "^1.7.0",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "0.0.32",
    "@types/express-jwt": "0.0.42",
    "@types/http-errors": "^1.6.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.136",
    "@types/lusca": "^1.6.0",
    "@types/nodemailer": "^6.2.0",
    "@types/validator": "^10.11.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dot-object": "^1.8.1",
    "dotenv": "^8.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-jwt-permissions": "^1.3.1",
    "express-validator": "^6.1.1",
    "http-errors": "^1.7.3",
    "lodash": "^4.17.15",
    "lusca": "^1.6.1",
    "nodemailer": "^6.3.0",
    "sequelize": "^5.10.1",
    "twilio": "^3.33.2"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3"
  }
}
